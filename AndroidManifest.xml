<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.frain.student"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="21" />

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/default_style" >
        <activity
            android:name="com.frain.student.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <!-- 新增其他Activity页面 -->

        <activity android:name="com.frain.student.activity.InterestringActivity" >
            <intent-filter
                android:icon="@drawable/labixiaoxin"
                android:label="哈哈" >
                <action android:name="com.frain.haha" />
                <!-- 如果不想设置范畴必须设置默认范畴 -->
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <!-- 设置具有访问http地址的范畴 -->
        <activity android:name="com.frain.student.activity.MainTwoActivity" >
            <intent-filter >
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <data
                    android:scheme="http" />
            </intent-filter>
        </activity>
    </application>

</manifest>